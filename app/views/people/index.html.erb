<p id="notice"><%= notice %></p>

<h1>People</h1>

<table>
  <thead>
    <tr>
      <th>F name</th>
      <th>M name</th>
      <th>L name</th>
      <th>Prefix</th>
      <th>Address1</th>
      <th>Address2</th>
      <th>City</th>
      <th>State</th>
      <th>Zipcode</th>
      <th>Education</th>
      <th>Estimated income</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @people.each do |person| %>
      <tr>
        <td><%= person.f_name %></td>
        <td><%= person.m_name %></td>
        <td><%= person.l_name %></td>
        <td><%= person.prefix %></td>
        <td><%= person.address1 %></td>
        <td><%= person.address2 %></td>
        <td><%= person.city %></td>
        <td><%= person.state %></td>
        <td><%= person.zipcode %></td>
        <td><%= person.education %></td>
        <td><%= person.estimated_income %></td>
        <td><%= link_to 'Show', person %></td>
        <td><%= link_to 'Edit', edit_person_path(person) %></td>
        <td><%= link_to 'Destroy', person, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Person', new_person_path %>
